# Stream Configuration
EXPO_PUBLIC_STREAM_API_KEY=your_stream_api_key_here
STREAM_API_SECRET=your_stream_api_secret_here

# Backend API URL (Replit deployed backend)
# For local development: http://localhost:3000
# For production on Expo.dev: https://your-replit-backend-url.replit.dev
EXPO_PUBLIC_API_URL=http://localhost:3000

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Expo Configuration (automatically set by Replit for dev)
EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN
REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN
